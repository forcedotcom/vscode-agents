{
  "name": "salesforcedx-vscode-agents",
  "displayName": "Agentforce DX",
  "description": "Provides a suite of tools to build, preview, and test agents",
  "version": "1.2.0",
  "publisher": "salesforce",
  "o11yUploadEndpoint": "https://794testsite.my.site.com/byolwr/webruntime/log/metrics",
  "enableO11y": "true",
  "repository": {
    "url": "https://github.com/salesforcecli/vsode-agents"
  },
  "icon": "images/agentforce-dx.png",
  "license": "BSD-3-Clause",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "main": "lib/src/extension.js",
  "dependencies": {
    "@salesforce/agents": "0.17.5",
    "@salesforce/core": "8.22.0",
    "@salesforce/types": "^1.3.0",
    "cross-spawn": "^7.0.6",
    "fast-xml-parser": "^4.5.1",
    "semver": "^7.7.2",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.6.0",
    "@salesforce/dev-config": "^4.3.1",
    "@tsconfig/node18": "^18.2.4",
    "@types/cross-spawn": "^6.0.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.19.74",
    "@types/semver": "7.5.8",
    "@types/sinon": "^17.0.3",
    "@types/vscode": "^1.90.0",
    "@types/vscode-webview": "^1.57.5",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@vscode/vsce": "^3.2.2",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "3.3.0",
    "esbuild": "0.25.8",
    "esbuild-plugin-pino": "^2.2.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-jsdoc": "46.8.2",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.4.2",
    "sinon": "^19.0.2",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typedoc": "^0.27.7",
    "typescript": "^5.7.3",
    "vscode-test": "^1.6.1",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "postinstall": "npm run install:combined",
    "install:combined": "cd agent-chat-and-trace && npm install",
    "build": "run-p build:main build:combined",
    "build:main": "tsc --build",
    "build:combined": "cd agent-chat-and-trace && npm run build",
    "build:clean": "tsc --build --clean",
    "compile": "npm run build",
    "compile:clean": "tsc --build --clean",
    "reinstall": "git checkout -- package-lock.json && git clean -xfd && npm install",
    "watch": "tsc --build --watch",
    "prettier": "prettier --write .",
    "prettier-check": "prettier --check .",
    "lint": "eslint . --ext .ts",
    "update-snapshots": "jest --updateSnapshot",
    "test": "jest --coverage",
    "bundle:extension": "node ./esbuild.config.js",
    "vscode:package": "npm run compile && npm run bundle:extension & npx vsce package",
    "vscode:prepublish": "npm run update-for-packaging",
    "update-for-packaging": "ts-node scripts/updatePackageJsonForVSIX.ts",
    "vsix:install": "find ./ -name '*.vsix' -exec code --install-extension {}",
    "vsix:install-insiders": "find ./ -name '*.vsix' -exec code-insiders --install-extension {}",
    "reformat": "node scripts/reformat-with-prettier.js"
  },
  "packaging": {
    "assets": [
      "LICENSE",
      "package.nls.json",
      "README.md",
      ".vscodeignore",
      "resources",
      "images",
      "dist"
    ],
    "packageUpdates": {
      "main": "dist/extension.js",
      "dependencies": {
        "shelljs": "0.8.5",
        "cross-spawn": "^7.0.6",
        "fast-xml-parser": "^4.5.1",
        "semver": "^7.7.0"
      },
      "devDependencies": {}
    }
  },
  "extensionDependencies": [
    "salesforce.salesforcedx-vscode-core",
    "salesforce.salesforcedx-vscode-apex-replay-debugger"
  ],
  "activationEvents": [
    "workspaceContains:sfdx-project.json"
  ],
  "contributes": {
    "configuration": {
      "title": "Salesforce",
      "properties": {
        "salesforce.agentforceDX.showGeneratedData": {
          "type": "boolean",
          "default": false,
          "description": "Show generated data in test results output"
        },
        "salesforce.agentforceDX.showAgentTracer(Mocked)": {
          "type": "boolean",
          "default": false,
          "description": "Show the mocked Agent Tracer tab in the Agent Preview panel"
        },
        "salesforce.agentforceDX.mockFile": {
          "type": "string",
          "description": "Absolute path to the mock agent trace response file"
        }
      }
    },
    "views": {
      "test": [
        {
          "id": "sf.agent.test.view",
          "name": "%agent_test_view_name%",
          "when": "sf:project_opened"
        }
      ],
      "agentforce-dx": [
        {
          "id": "sf.agent.combined.view",
          "name": "%agent_combined%",
          "type": "webview",
          "when": "sf:project_opened"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "agentforce-dx",
          "title": "Agentforce DX",
          "icon": "images/activity-bar-icon.svg"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "sf.agent.test.view.runAll",
          "when": "never && view == sf.agent.test.view",
          "group": "navigation@1"
        },
        {
          "command": "sf.agent.test.view.refresh",
          "when": "view == sf.agent.test.view",
          "group": "navigation@2"
        },
        {
          "command": "sf.agent.test.view.collapseAll",
          "when": "view == sf.agent.test.view",
          "group": "navigation@3"
        },
        {
          "command": "sf.agent.test.view.toggleGeneratedData.on",
          "when": "view == sf.agent.test.view && agentforceDX:showGeneratedData",
          "group": "navigation@4"
        },
        {
          "command": "sf.agent.test.view.toggleGeneratedData.off",
          "when": "view == sf.agent.test.view && !agentforceDX:showGeneratedData",
          "group": "navigation@4"
        }
      ],
      "view/item/context": [
        {
          "command": "sf.agent.test.view.runTest",
          "when": "view == sf.agent.test.view && viewItem =~ /agentTestGroup/",
          "group": "inline"
        },
        {
          "command": "sf.agent.test.view.goToTestResults",
          "when": "view == sf.agent.test.view && viewItem =~ /(agentTest|agentTestGroup)(_Pass|_Skip|\\b)/"
        }
      ],
      "editor/context": [
        {
          "command": "salesforcedx-vscode-agents.openAgentInOrg",
          "when": "resourceFilename =~ /.*\\.bot-meta\\.xml$/ || resourceFilename =~ /.*\\.genAiPlannerBundle$/ ||  resourceFilename =~ /.*\\.botVersion-meta\\.xml$/ || resourceFilename =~ /.*Planner\\.genAiPlannerBundle$/ || resourceFilename =~ /.*\\.genAiPlanner-meta\\.xml$/",
          "group": "agent@1"
        },
        {
          "command": "salesforcedx-vscode-agents.activateAgent",
          "when": "resourceFilename =~ /.*\\.bot-meta\\.xml/ || resourceFilename =~ /.*\\.genAiPlannerBundle$/ || resourceFilename =~ /.*\\.genAiPlannerBundle/ || resourceFilename =~ /.*\\.botVersion-meta\\.xml/",
          "group": "agent@2"
        },
        {
          "command": "salesforcedx-vscode-agents.deactivateAgent",
          "when": "resourceFilename =~ /.*\\.bot-meta\\.xml/ || resourceFilename =~ /.*\\.genAiPlannerBundle$/ || resourceFilename =~ /.*\\.genAiPlannerBundle/ || resourceFilename =~ /.*\\.botVersion-meta\\.xml/",
          "group": "agent@3"
        }
      ],
      "explorer/context": [
        {
          "command": "salesforcedx-vscode-agents.openAgentInOrg",
          "when": "resourceFilename =~ /.*\\.bot-meta\\.xml/ || resourceFilename =~ /.*\\.genAiPlannerBundle$/ || resourceFilename =~ /.*\\.genAiPlannerBundle/ || resourceFilename =~ /.*\\.botVersion-meta\\.xml/ || resourceFilename =~ /.*\\.genAiPlanner-meta\\.xml/",
          "group": "agent@1"
        },
        {
          "command": "salesforcedx-vscode-agents.activateAgent",
          "when": "resourceFilename =~ /.*\\.bot-meta\\.xml/ || resourceFilename =~ /.*\\.genAiPlannerBundle$/ || resourceFilename =~ /.*\\.genAiPlannerBundle/ || resourceFilename =~ /.*\\.botVersion-meta\\.xml/",
          "group": "agent@2"
        },
        {
          "command": "salesforcedx-vscode-agents.deactivateAgent",
          "when": "resourceFilename =~ /.*\\.bot-meta\\.xml/ || resourceFilename =~ /.*\\.genAiPlannerBundle$/ || resourceFilename =~ /.*\\.genAiPlannerBundle/ || resourceFilename =~ /.*\\.botVersion-meta\\.xml/",
          "group": "agent@3"
        }
      ],
      "commandPalette": [
        {
          "command": "sf.agent.test.view.goToTestResults",
          "when": "false"
        },
        {
          "command": "salesforcedx-vscode-agents.openAgentInOrg",
          "when": "resourceFilename =~ /.*\\.bot-meta\\.xml/ || resourceFilename =~ /.*\\.botVersion-meta\\.xml/ || resourceFilename =~ /.*\\.genAiPlanner-meta\\.xml/"
        },
        {
          "command": "sf.agent.test.view.runAll",
          "when": "false"
        },
        {
          "command": "sf.agent.test.view.runTest",
          "when": "false"
        },
        {
          "command": "sf.agent.test.view.refresh",
          "when": "false"
        },
        {
          "command": "sf.agent.test.view.collapseAll",
          "when": "sf:project_opened"
        },
        {
          "command": "sf.agent.openChat",
          "when": "true"
        }
      ]
    },
    "commandPalette": [
      {
        "command": "salesforcedx-vscode-agents.openAgentInOrg",
        "when": "resourceFilename =~ /.*\\.bot-meta\\.xml/ || resourceFilename =~ /.*\\.botVersion-meta\\.xml/ || resourceFilename =~ /.*\\.genAiPlanner-meta\\.xml/"
      },
      {
        "command": "salesforcedx-vscode-agents.activateAgent",
        "when": "resourceFilename =~ /.*\\.bot-meta\\.xml/ || resourceFilename =~ /.*\\.botVersion-meta\\.xml/"
      },
      {
        "command": "salesforcedx-vscode-agents.deactivateAgent",
        "when": "resourceFilename =~ /.*\\.bot-meta\\.xml/ || resourceFilename =~ /.*\\.botVersion-meta\\.xml/"
      },
      {
        "command": "sf.agent.test.view.runAll",
        "when": "false"
      },
      {
        "command": "sf.agent.test.view.runTest",
        "when": "false"
      },
      {
        "command": "sf.agent.test.view.refresh",
        "when": "false"
      },
      {
        "command": "sf.agent.test.view.collapseAll",
        "when": "sf:project_opened"
      }
    ],
    "commands": [
      {
        "command": "sf.agent.test.view.goToTestResults",
        "title": "%go_to_test_results%"
      },
      {
        "command": "sf.agent.test.view.refresh",
        "title": "%refresh_test_title%",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "sf.agent.test.view.collapseAll",
        "title": "%collapse_tests_title%",
        "icon": {
          "light": "resources/light/collapse-all.svg",
          "dark": "resources/dark/collapse-all.svg"
        }
      },
      {
        "command": "sf.agent.test.view.runAll",
        "title": "%run_all_tests_title%",
        "icon": {
          "light": "resources/light/play-button.svg",
          "dark": "resources/dark/play-button.svg"
        }
      },
      {
        "command": "sf.agent.test.view.runTest",
        "title": "%run_test_title%",
        "icon": {
          "light": "resources/light/play-button.svg",
          "dark": "resources/dark/play-button.svg"
        }
      },
      {
        "command": "salesforcedx-vscode-agents.openAgentInOrg",
        "title": "%salesforcedx-vscode-agents.openAgentInOrg%"
      },
      {
        "command": "sf.agent.openChat",
        "title": "%open_chat_with_agent%",
        "icon": "$(chat-editor-label-icon)"
      },
      {
        "command": "salesforcedx-vscode-agents.activateAgent",
        "title": "%salesforcedx-vscode-agents.activateAgent%"
      },
      {
        "command": "salesforcedx-vscode-agents.deactivateAgent",
        "title": "%salesforcedx-vscode-agents.deactivateAgent%"
      },
      {
        "command": "sf.agent.test.view.toggleGeneratedData.on",
        "title": "Hide Generated Data Output",
        "category": "Agent Tests",
        "icon": {
          "light": "resources/light/check-on.svg",
          "dark": "resources/dark/check-on.svg"
        }
      },
      {
        "command": "sf.agent.test.view.toggleGeneratedData.off",
        "title": "Show Generated Data Output",
        "category": "Agent Tests",
        "icon": {
          "light": "resources/light/check-off.svg",
          "dark": "resources/dark/check-off.svg"
        }
      }
    ]
  }
}
